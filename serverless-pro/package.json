{
  "name": "serverless-pro",
  "version": "0.1.0",
  "bin": {
    "serverless-pro": "bin/pipeline.js"
  },
  "scripts": {
    "synth": "cdk synth --all",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "deploy:pipeline": "cdk deploy ServerlessPro --profile=whatsapptracking-cdk",
    "remove:pipeline": "cdk destroy ServerlessPro --profile=whatsapptracking-cdk",
    "deploy:dev": "cdk deploy ServerlessPro/Develop-PR-123/StatefulStack ServerlessPro/Develop-PR-123/StatelessStack --profile=whatsapptracking-cdk",
    "preremove:dev": "cdk synth --profile=whatsapptracking-cdk",
    "remove:dev": "cdk destroy ServerlessPro/Develop-PR-123/StatefulStack ServerlessPro/Develop-PR-123/StatelessStack --profile=whatsapptracking-cdk"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.110",
    "@types/jest": "^29.2.4",
    "@types/node": "18.11.15",
    "@types/uuid": "^9.0.0",
    "aws-cdk": "2.55.1",
    "esbuild": "^0.17.5",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.4"
  },
  "dependencies": {
    "aws-cdk-lib": "2.55.1",
    "aws-sdk": "^2.1304.0",
    "constructs": "^10.0.0",
    "dotenv": "^16.0.3",
    "source-map-support": "^0.5.21",
    "uuid": "^9.0.0"
  }
}
